{
  "name": "scrypted-amcrest-ash21",
  "version": "1.0.5",
  "description": "Amcrest ASH21 PTZ Camera Plugin for Scrypted - Full PTZ control via DVRIP protocol",
  "author": "Joshua Blasbalg",
  "license": "MIT",
  "main": "dist/main.nodejs.js",
  "keywords": [
    "scrypted",
    "amcrest",
    "ash21",
    "ptz",
    "camera",
    "dahua",
    "dvrip",
    "homekit",
    "smart home"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/jblasbalg/scrypted-amcrest-ash21"
  },
  "files": [
    "dist/main.nodejs.js",
    "dist/main.nodejs.js.map",
    "dist/sdk.json",
    "README.md"
  ],
  "scripts": {
    "build": "scrypted-webpack",
    "watch": "scrypted-webpack --watch",
    "scrypted-deploy": "scrypted-deploy",
    "scrypted-debug": "scrypted-debug",
    "deploy": "npm run build && npm run scrypted-deploy",
    "prepublishOnly": "npm run build"
  },
  "scrypted": {
    "name": "Amcrest ASH21 PTZ",
    "type": "DeviceProvider",
    "interfaces": [
      "DeviceProvider",
      "Settings"
    ],
    "deviceInterfaces": [
      "Camera",
      "VideoCamera",
      "PanTiltZoom",
      "Settings"
    ]
  },
  "devDependencies": {
    "@scrypted/sdk": "^0.5.55",
    "@types/node": "^20.0.0",
    "ts-loader": "^9.5.0",
    "typescript": "^5.0.0",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  }
}
